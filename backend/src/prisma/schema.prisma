datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id            String   @id @default(uuid())
  email         String   @unique
  name          String
  auth_platform String   @default("local")
  auth_id       String?
  password_hash String?
  created_at    DateTime @default(now())
}
